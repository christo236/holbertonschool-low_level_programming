#include <stdlib.h>
#include <time.h>

/**
 * main - The last digit
 *
 * print le dernier chiffre du nombre stockÃ dans la variable n
 *
 * Return: Always 0.
 *
 */
int main(void)
{
printf("Last digit of %d is %d", and is less than 6 and not 0\n"
/* Affiche last ... n of n reste ...*/
, n, n % 10); /* remplace "%d" par n et reste n*/
}
else if ((n % 10) > 5) /* si le rest n est plus grand que 5*/
}
printf( "Last digit of %d is %d and greater than 5\n"
/* Affiche last ... n of n rest plus grand que 5*/
, n, n % 10) ; /* remplace %d par n et rest n*/
}
else /*Sinon*/
{
printf( "Last digit of %d is %d and is 0\n"
/* Affiche last ... n is n rest et 0*/
, n, n % 10) ;
}

return (0);
}
